<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.facepay_ui">

    <application
        android:allowBackup="true"
        android:icon="@drawable/face_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Transaction_complete"></activity>
        <activity android:name=".Mobile_number" />
        <activity android:name=".Payment" />
        <activity android:name=".FaceRecognition" />
        <activity android:name=".Fingerprint" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>